{
    "name": "qobo/cakephp-messaging-center",
    "description": "Messaging Center plugin for CakePHP",
    "type": "cakephp-plugin",
    "license": "GPL-2.0",
    "require": {
        "php": ">=5.4.16",
        "cakephp/cakephp": "~3.0",
        "cakedc/users": "~3.1"
    },
    "require-dev": {
        "phpunit/phpunit": "*"
    },
    "autoload": {
        "psr-4": {
            "MessagingCenter\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MessagingCenter\\Test\\": "tests",
            "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
        }
    }
}
